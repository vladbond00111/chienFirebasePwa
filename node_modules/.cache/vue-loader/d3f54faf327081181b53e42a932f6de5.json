ate>\n\n<script>\n  import firebase from '../configFirebase.js'\n  export default {\n    data(){\n      return{\n        dogs:[]\n      }\n    },\n    mounted(){\n      firebase.db.collection('dogs').orderBy('created_at').onSnapshot((snapShot) => {\n        this.dogs=[];\n        snapShot.forEach((dog)  => {\n          this.dogs.push({\n            id:dog.id,\n            url:dog.data().url,\n            comment:dog.data().comment,\n            info:dog.data().info\n          })\n        });\n      });\n    }\n  }\n</script>\n"]}]}