ls', params:{ id:dog.id, dogProp:dogs[index] }})\">\n          <v-img\n            height=\"170\"\n            :src=\"dog.url\"\n            aspect-ratio=\"2.75\"\n          ></v-img>\n          <v-card-title primary-title style=\"padding-top:13px\">\n            <div>\n              <h3 class=\"headline\">{{ dog.comment }}</h3>\n              <div> {{ dog.info }}</div>\n            </div>\n          </v-card-title>\n        </v-card>\n      </v-flex>\n    </v-layout>\n    <v-btn @click=\"$router.push({ name: 'post'})\"  color=\"red\" dark fixed bottom right fab>\n      <v-icon>add</v-icon>\n    </v-btn>\n  </v-container>\n</template>\n\n<script>\n  import firebase from '../configFirebase.js'\n  export default {\n    data(){\n      return{\n        dogs:[]\n      }\n    },\n    mounted(){\n      firebase.db.collection('dogs').orderBy('created_at').onSnapshot((snapShot) => {\n        this.dogs=[];\n        snapShot.forEach((dog)  => {\n          this.dogs.push({\n            id:dog.id,\n            url:dog.data().url,\n            comment:dog.data().comment,\n            info:dog.data().info\n          })\n        });\n      });\n    }\n  }\n</script>\n"],"sourceRoot":"src/components"}]}